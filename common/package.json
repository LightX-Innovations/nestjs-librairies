{
  "name": "@recursyve/nestjs-common",
  "version": "1.0.0",
  "description": "Recursyve's NestJs common librairy",
  "main": "index.js",
  "scripts": {
    "watch": "gulp",
    "build": "gulp build",
    "build:lib": "gulp build-lib",
    "lint": "gulp lint",
    "publish-package": "npm run build:lib && npm publish dist --access public",
