  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.14.16",
    "gulp": "^4.0.2",
    "gulp-copy": "^4.0.1",
