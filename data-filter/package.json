{
    "name": "@lightx-innovations/nestjs-data-filter",
    "version": "10.2.3",
    "description": "NestJs DataFilter library",
    "main": "index.js",
    "scripts": {
        "watch": "gulp",
        "build": "gulp build",
        "build:lib": "gulp build-lib",
        "lint": "gulp lint",
        "publish-package": "npm run build:lib && npm publish ./dist --access public",
        "publish-package:beta": "npm run publish-package -- --tag beta --access public",
        "publish-package:dry-run": "npm run publish-package -- --dry-run",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LightX-Innovations/nestjs-librairies/data-filter"
    },
    "author": "Recursyve",
    "license": "MIT",
    "peerDependencies": {
        "@lightx-innovations/nestjs-common": ">=10.1.0",
        "@nestjs/common": ">=10.0.0",
        "@nestjs/core": ">=10.0.0",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4",
        "sequelize": ">=6.35.2",
        "sequelize-typescript": ">=2.1.6"
    },
    "devDependencies": {
        "@lightx-innovations/nestjs-common": "^10.1.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "date-fns": "^2.30.0",
        "date-fns-tz": "^2.0.0",
        "express": "^4.18.2",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^29.7.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "sequelize": "6.35.2",
        "sequelize-typescript": "2.1.6",
        "sqlite3": "^5.1.4",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    },
    "dependencies": {
        "csv-stringify": "^5.6.2",
        "xlsx": "^0.17.2"
