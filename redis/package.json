{
    "name": "@lightx-innovations/nestjs-redis",
    "version": "10.1.0",
    "description": "NestJs Redis client",
    "main": "index.js",
    "scripts": {
        "watch": "gulp",
        "build": "gulp build",
        "build:lib": "gulp build-lib",
        "lint": "gulp lint",
        "publish-package": "npm run build:lib && npm publish ./dist --access public",
        "publish-package:beta": "npm run publish-package -- --tag beta --access public",
        "publish-package:dry-run": "npm run publish-package -- --dry-run",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LightX-Innovations/nestjs-librairies/redis"
    },
    "author": "Recursyve",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": ">=10.0.0",
        "@nestjs/core": ">=10.0.0",
        "@nestjs/terminus": ">=10.0.0"
    },
    "dependencies": {
        "ioredis": "^5.3.2"
    },
    "devDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/terminus": "^10.0.0",
        "@types/jest": "^29.2.3",
        "@nestjs/testing": "^10.0.0",
        "@types/node": "^16.11.22",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^29.3.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.6.0",
        "ts-jest": "^29.0.3",
        "typescript": "^4.5.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
